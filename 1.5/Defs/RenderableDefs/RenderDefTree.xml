										<dataEast>
											<offset>(-0.3,0.001,0.1)</offset>
											<layer>1</layer>
										</dataEast>
										<dataSouth>
											
										</dataSouth>
										<dataWest>
											<offset>(0.3,-0.001,0.1)</offset>
										</dataWest>
										<dataNorth>
											<layer>1</layer>
										</dataNorth>
									</drawData>
								</li>
								<li Class="RVCRestructured.RVR.RenderableDefNodeProperties">
									<def>RVFFA_EarLU</def>
									<workerClass>RVCRestructured.RVR.RNodeWorker</workerClass>
									<nodeClass>RVCRestructured.RVR.RenderableDefNode</nodeClass>
									<drawData>
										<dataEast>
											<offset>(-0.3,-0.001,0)</offset>
											<layer>-1</layer>
										</dataEast>
										<dataSouth>
											<layer>0.001</layer>
										</dataSouth>
										<dataWest>
											<offset>(0.3,0.001,0)</offset>
										</dataWest>
										<dataNorth>
											<layer>1</layer>
										</dataNorth>
									</drawData>
								</li>
								<li Class="RVCRestructured.RVR.RenderableDefNodeProperties">
									<def>RVFFA_EarRU</def>
									<workerClass>RVCRestructured.RVR.RNodeWorker</workerClass>
									<nodeClass>RVCRestructured.RVR.RenderableDefNode</nodeClass>
									<drawData>

										<dataEast>
											<offset>(-0.3,0.001,0)</offset>
											<layer>1</layer>
										</dataEast>
										<dataSouth>
											<layer>0.001</layer>
										</dataSouth>
										<dataWest>
											<offset>(0.3,-0.001,0)</offset>
										</dataWest>
										<dataNorth>
											<layer>1</layer>
										</dataNorth>
									</drawData>
								</li>
								
								<li Class="RVCRestructured.RVR.RenderableDefNodeProperties">
									<def>RVFFA_Mouth</def>
									<workerClass>RVCRestructured.RVR.RNodeWorker</workerClass>
									<nodeClass>RVCRestructured.RVR.RenderableDefNode</nodeClass>
									<drawData>

										<dataEast>
											<layer>1</layer>
										</dataEast>
										<dataSouth>
											<layer>1</layer>
										</dataSouth>
									</drawData>
								</li>
								<li Class="RVCRestructured.RVR.RenderableDefNodeProperties">
									<def>RVFFA_EyeL</def>
									<workerClass>RVCRestructured.RVR.RNodeWorker</workerClass>
									<nodeClass>RVCRestructured.RVR.RenderableDefNode</nodeClass>
									<drawData>

										<dataEast>
											<layer>1</layer>
										</dataEast>
										<dataSouth>
											<layer>1</layer>
										</dataSouth>
										<dataWest>
											<layer>1</layer>
										</dataWest>
									</drawData>
								</li>
								<li Class="RVCRestructured.RVR.RenderableDefNodeProperties">
									<def>RVFFA_EyeR</def>
									<workerClass>RVCRestructured.RVR.RNodeWorker</workerClass>
									<nodeClass>RVCRestructured.RVR.RenderableDefNode</nodeClass>
									<drawData>

										<dataEast>
											<layer>1</layer>
										</dataEast>
										<dataSouth>
											<layer>1</layer>
										</dataSouth>
										<dataWest>
											<layer>1</layer>
										</dataWest>
									</drawData>
								</li>
							</children>
						</li>
						<li Class="RVCRestructured.RVR.RenderableDefNodeProperties">
							<def>RVFFA_TailSkirt</def>
							<workerClass>RVCRestructured.RVR.RNodeWorker</workerClass>
							<nodeClass>RVCRestructured.RVR.RenderableDefNode</nodeClass>
							<drawData>
								<dataEast>
									<offset>(-0.25,0.001,-0.285)</offset>
								</dataEast>
								<dataNorth>
									<layer>1</layer>
								</dataNorth>
							</drawData>
							<children>
								<li Class="RVCRestructured.RVR.RenderableDefNodeProperties">
									<def>RVFFA_Tail</def>
									<workerClass>RVCRestructured.RVR.RNodeWorker</workerClass>
									<nodeClass>RVCRestructured.RVR.RenderableDefNode</nodeClass>
									<drawData>
										<dataEast>
											<offset>(-0.5,0,0.5)</offset>
										</dataEast>
										<dataNorth>
											<offset>(-0.15,1.5,0.1)</offset>

										</dataNorth>
										<dataSouth>
											<offset>(0.3,0,0.1)</offset>
										</dataSouth>
									</drawData>
								</li>
							</children>
						</li>
						
					</children>
				</li>
				
				<li Class="PawnRenderNodeProperties_Carried">
					<debugLabel>Weapon/carried thing</debugLabel>
					<workerClass>PawnRenderNodeWorker_Carried</workerClass>
				</li>
			</children>
		</root>
	</PawnRenderTreeDef>
</Defs>