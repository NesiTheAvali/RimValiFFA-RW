<Defs>

  <ThingDef ParentName="MealLavishBase">
    <defName>RVFFA_Taco</defName>
    <label>Avali tacos</label>
    <description>Avali tacos.</description>
    <graphicData>
      <texPath>RimValiFFA/Foods/tacos</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
     <statBases>
      <MarketValue>40</MarketValue>
      <WorkToMake>730</WorkToMake>
      <Nutrition>1</Nutrition>
    </statBases>
    <ingestible>
      <preferability>MealLavish</preferability>
      <optimalityOffsetHumanlikes>2</optimalityOffsetHumanlikes>
      <optimalityOffsetFeedingAnimals>-10</optimalityOffsetFeedingAnimals>
    </ingestible>
  </ThingDef>


