  <ThingDef ParentName="MealBase">
    <defName>RVFFA_Jerky</defName>
    <label>Avali jerky</label>
    <description>Avali jerky.</description>
    <graphicData>
      <texPath>RimValiFFA/Foods/jerky</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <statBases>
      <MarketValue>1.4</MarketValue>
      <Mass>0.1</Mass>
      <Flammability>0.6</Flammability>
      <WorkToMake>650</WorkToMake>
      <DeteriorationRate>2</DeteriorationRate>
      <Nutrition>0.15</Nutrition>
    </statBases>
    <ingestible>
      <preferability>MealSimple</preferability>
      <optimalityOffsetHumanlikes>-5</optimalityOffsetHumanlikes>
      <optimalityOffsetFeedingAnimals>-10</optimalityOffsetFeedingAnimals>
      <ingestEffect>EatVegetarian</ingestEffect>
      <ingestSound>Meal_Eat</ingestSound>
    </ingestible>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>70</daysToRotStart>
        <rotDestroys>true</rotDestroys>
      </li>
    </comps>
  </ThingDef> 


